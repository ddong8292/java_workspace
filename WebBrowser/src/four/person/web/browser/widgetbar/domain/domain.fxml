<?import javafx.scene.control.Button?>

<HBox xmlns:fx="http://javafx.com/fxml" prefHeight="40.0" fx:id="hboxDomain"
	fx:controller="four.person.web.browser.widgetbar.domain.DomainController"
	alignment="CENTER_LEFT"
	stylesheets="@domain.css"
	spacing="5.0">
	<padding>
		<Insets left="10.0" right="10.0"></Insets>
	</padding>
	<children>
		<TextField fx:id="tfDomain" prefHeight="30.0" id="tf_domain"/>
		<Button fx:id="btnExit" prefHeight="20.0" prefWidth="20.0" id="btn_exit" alignment="TOP_CENTER"/>
	</children>
</HBox>