<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ScrollPane?>

<ScrollPane 
	xmlns:fx="http://javafx.com/fxml"
	fx:controller="four.person.web.browser.settings.basic.SettingsBasicController"
	id="scroll">
	<content>
		<VBox id="vbox_content">
			<children>
				<Label text="시작  홈페이지" id="label_title" />
				<VBox id="vbox_inner">
					<children>
						<RadioButton fx:id="radBtnDefault" id="content_title" text="기본" />
						<RadioButton fx:id="radBtnCustom" id="content_title" text="직접입력" />
						<TextField fx:id="tfDomain" id="content_title" prefWidth="300.0" maxWidth="300.0" minWidth="300.0" disable="false" />
					</children>
				</VBox>
			</children>
		</VBox>
	</content>
</ScrollPane>
